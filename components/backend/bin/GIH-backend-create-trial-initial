sensor_temperatura_ambiente_1 = SensorCommon(TipoSensor.TEMPERATURA,ZonaSensor.AMBIENTE,1,ModeloSensor.LM35,
                                         direccion_lectura="MCP3008",patilla_0_lectura=4, 
                                         unidad_medida_0=UnidadMedida.GRADOS_CENTIGRADOS)
sensor_temperatura_ambiente_1 =SensorService.createFromCommon(db,sensor_temperatura_ambiente_1)
sensor_humedad_ambiente_1 = SensorCommon(TipoSensor.HUMEDAD,ZonaSensor.AMBIENTE,1,ModeloSensor.OTRO,
                                         direccion_lectura="MCP3008",patilla_0_lectura=5, 
                                         unidad_medida_0=UnidadMedida.PORCENTAJE)
sensor_humedad_ambiente_1 =SensorService.createFromCommon(db,sensor_humedad_ambiente_1)



